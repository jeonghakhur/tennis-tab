# Supabase 설정
# Supabase 대시보드 > Settings > API 에서 확인
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# Site URL (OAuth 리다이렉트용)
NEXT_PUBLIC_SITE_URL=http://localhost:3000

# OpenAI API (자연어 처리용 - Phase 1에서 설정)
OPENAI_API_KEY=sk-your-openai-api-key

# 카카오 OAuth (Supabase 지원)
# Supabase 대시보드에서 설정: Authentication > Providers > Kakao
# - Client ID: REST API Key
# - Client Secret: Admin Key

# 네이버 OAuth (직접 구현 - Supabase 미지원)
NAVER_CLIENT_ID=your_naver_client_id
NAVER_CLIENT_SECRET=your_naver_client_secret

# 네이버 지도 API
# 네이버 클라우드 플랫폼 > AI·NAVER API > Maps > Application 에서 발급
NEXT_PUBLIC_NAVER_MAP_CLIENT_ID=your_naver_map_client_id
